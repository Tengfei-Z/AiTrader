# Core configuration
APP_ENV=development
LOG_LEVEL=INFO

# DeepSeek credentials
DEEPSEEK_API_KEY=sk-****
DEEPSEEK_API_BASE=https://api.deepseek.com

# OKX credentials
OKX_API_KEY=
OKX_SECRET_KEY=
OKX_PASSPHRASE=
OKX_BASE_URL=https://www.okx.com
OKX_USE_SIMULATED=1
OKX_HTTP_MAX_RETRIES=3
OKX_HTTP_RETRY_BACKOFF=0.5
# Comma-separated instrument ids to sync (order matters)
OKX_INST_IDS=BTC-USDT-SWAP

VITE_APP_BRAND_NAME=NovaTrade AI
VITE_APP_BRAND_TAGLINE=智能量化交易平台


# FastAPI
AGENT_HOST=0.0.0.0
AGENT_PORT=8001
AGENT_BASE_URL=ws://localhost:8001/agent/events/ws

# Strategy scheduler
STRATEGY_SCHEDULE_ENABLED=false
STRATEGY_SCHEDULE_INTERVAL_SECS=300
STRATEGY_MANUAL_TRIGGER_ENABLED=false

# Initial equity (default: 122000.0)
INITIAL_EQUITY=122000.0
BALANCE_SNAPSHOT_MIN_ABS_CHANGE=2.0
BALANCE_SNAPSHOT_MIN_RELATIVE_CHANGE=0.0005

# Database reset (WARNING: Setting to 'true' will DELETE ALL DATA on startup!)
# 数据库重置（警告：设置为 'true' 将在启动时删除所有数据！）
RESET_DATABASE=false
DATABASE_URL=postgres://user:password@127.0.0.1:5432/aitrader
DATABASE_SCHEMA=aitrader
